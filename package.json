{
  "name": "jsxhint",
  "version": "0.3.0",
  "description": "Wrapper for JSHint to allow hinting of JSX files",
  "main": "jsxhint.js",
  "bin": "./cli.js",
  "scripts": {
    "test": "tap test/test.js",
    "lint": "jshint",
    "postinstall": "if [[ -d .git ]]; then cp pre-commit.sh .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit; fi",
    "precommit": "npm run lint && npm test",
    "prepublish": "docme README.md-- --configure ./.jsdocrc.json"
  },
  "repository": {
    "type": "git",
    "url": "CondeNast/JSXHint"
  },
  "prefer_global": true,
  "keywords": [
    "jshint",
    "jsx",
    "react",
    "lint",
    "jslint",
    "reactjs"
  ],
  "author": {
    "name": "Todd Kennedy",
    "email": "<todd@selfassembled.org>",
    "url": "http://tck.io"
  },
  "license": "MIT",
  "dependencies": {
    "runnel": "~0.5.1",
    "glob": "~3.2.6",
    "react-tools": "~0.4.1",
    "argparse": "~0.1.15",
    "colors": "~0.6.2",
    "mutiny": "~0.3.1",
    "through2": "~0.2.3",
    "minimist": "0.0.5",
    "tmp": "0.0.23",
    "rimraf": "~2.2.5"
  },
  "devDependencies": {
    "tap": "~0.4.6",
    "docme": "~0.1.4"
  }
}
